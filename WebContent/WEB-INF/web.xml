<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>member</servlet-name>
    <servlet-class>cloudservice.Membership</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>cloudservice.Login</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>cloudservice.Logout</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>schedule</servlet-name>
    <servlet-class>cloudservice.ScheduleHandler</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
   <servlet>
    <servlet-name>PhoneQuerier</servlet-name>
    <servlet-class>cloudservice.PhoneQuerier</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>member</servlet-name>
    <url-pattern>/member.register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/member.login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/member.logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>schedule</servlet-name>
    <url-pattern>/schedule.update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PhoneQuerier</servlet-name>
    <url-pattern>/phone.get</url-pattern>
  </servlet-mapping>
</web-app>